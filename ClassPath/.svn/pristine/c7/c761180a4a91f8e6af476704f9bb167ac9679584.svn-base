<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.or.ddit.pfcp.student.counsel.mapper.StudentCounselMapper">
	<insert id="insertStudentCounsel">
		INSERT INTO COUNSEL_REQ (
			COUNSEL_REQNO, 
			PREFERRED_DATE, 
			TYPE_CODE, 
			USER_NO, 
			STATUS, 
			COUNSEL_REQDATE, 
			COUNSEL_COMMENT)
		VALUES (
			#{counselReqno},
			#{preferredDate},
			#{typeCode},
			#{userNo},
			#{status},
			#{counselReqdate},
			#{counselComment})
	</insert>

	<select id="selectStudentCounselList">
		SELECT COUNSEL_REQNO, 
		       PREFERRED_DATE, 
		       TYPE_CODE, 
		       STUDENT_NO, 
		       STATUS, 
		       COUNSEL_REQDATE, 
		       COUNSEL_COMMENT
		  FROM COUNSEL_REQ
	</select>
	
	<select id="selectStudentCounsel">
		
	</select>
</mapper>