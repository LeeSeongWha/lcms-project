<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.or.ddit.pfcp.staff.college.mapper.GraduationRequestMapper">
	<select id="selectAllGraduationRequestList" resultType="kr.or.ddit.pfcp.common.vo.GraduationRequestVO">
		SELECT
			GRAD_REQ_NO
			,USER_NO
			,SEMESTER_NO
			,REQ_DATE
			,GRAD_STATUS
			,COM
		FROM GRADUATION_REQUEST
	</select>
</mapper>