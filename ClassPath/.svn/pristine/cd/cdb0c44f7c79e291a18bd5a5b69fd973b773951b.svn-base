<%--
 * == 개정이력(Modification Information) ==
 * 수정일	수정자	수정내용
 * ========================================
 * 250702	서경덕	최초 생성
--%>
<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>

<title>Insert title here</title>

<form method="post" action="/student/counsel/departmentCounsel/departmentCounselInsertProcess.do">
	<div class="form-group">
		<label class="form-label" for="studentNo">학번</label>
		<input type="text" id="studentNo" name="studentNo" class="form-control" value="${userNo}" readonly>
	</div>
	
	<div class="form-group">
		<label class="form-label" for="preferredDate">희망 상담 일자</label>
		<input type="datetime-local" id="preferredDate" name="preferredDate" class="form-control" placeholder="희망 상담 일자">
	</div>
	
	<div class="form-group">
		<label class="form-label" for="typeCode">유형 코드</label>
		<input type="text" id="typeCode" name="typeCode" class="form-control" placeholder="유형 코드">
		<!-- 
		<select name="typeCode" id="typeCode" class="form-select" data-init-val="${counselReq.typeCode }">
			<option value="">학과 코드</option>
		</select>  
		-->
	</div>
	
	<div class="form-group">
		<label class="form-label" for="counselReqdate">상담 신청 일시</label>
		<input type="text" id="counselReqdate" name="counselReqdate" class="form-control" placeholder="상담 신청 일시">
	</div>
	
	<div class="form-group">
		<label class="form-label" for="counselCommnet">상담 내용</label>
		<input type="text" id="counselCommnet" name="counselCommnet" class="form-control" placeholder="상담 내용(간단히 기재)">
	</div>
	
	<button class="btn btn-primary" type="submit">저장</button>
	<button class="btn btn-danger" type="reset">취소</button>
</form>