package kr.or.ddit.pfcp.staff.consult.controller;

import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.GetMapping;
import org.springframework.web.bind.annotation.RequestMapping;

/**
 * @author YSM
 * @since 250630
 */
@Controller
@RequestMapping("/staff/consult")
public class ConsultController {
	
	
	/**
	 * 상담 스케줄 목록 전체 조회
	 * @return
	 */
	@GetMapping("consultScheduleList.do")
	public String consultScheduleListUI() {
		return "pfcp/staff/consult/consultScheduleList";
	}
	
	/**
	 * 상담 스케줄 상세 조회
	 * @return
	 */
	@GetMapping("consultScheduleDetail.do")
	public String consultScheduleDetailUI() {
		return "pfcp/staff/consult/consultScheduleDetail";
	}
	
	/**
	 * 상담 스케줄 등록
	 * @return
	 */
	@GetMapping("consultScheduleRegist.do")
	public String consultScheduleInsertUI() {
		return "pfcp/staff/consult/consultScheduleRegist";
	}
	
	/**
	 * 상담 스케줄 수정
	 * @return
	 */
	@GetMapping("consultScheduleUpdate.do")
	public String consultScheduleUpdateUI() {
		return "pfcp/staff/consult/consultScheduleUpdate";
	}
}
