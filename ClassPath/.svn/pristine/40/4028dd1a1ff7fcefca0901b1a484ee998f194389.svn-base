<%--
 * == 개정이력(Modification Information) ==
 * 수정일	수정자	수정내용
 * ========================================
 * 250701	서경덕	최초 생성
--%>
<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>

<title>교수 수정</title>

<div class="card">
	<div class="card-header">
		<h5>교수 수정</h5>
	</div>
	<div class="card-body">
		<div class="row">
			<form method="post" action="/staff/professorManagement/professorUpdateProcess.do">
				<div class="form-group">
					<label class="form-label" for="userNo">교번</label>
					<input type="text" id="userNo" name="user.userNo" class="form-control" value="${professor.user.userNo }" readonly>
				</div>
				
				<div class="form-group">
					<label class="form-label" for="userName">교수명</label>
					<input type="text" id="userName" name="user.userName" class="form-control" value="${professor.user.userName }">
				</div>
				
				<div class="form-group">
					<label class="form-label" for="proPosition">교수 직책</label>
					<input type="text" id="proPosition" name="proPosition" class="form-control" value="${professor.proPosition }">
				</div>
				<div class="form-group">
					<label class="form-label" for="departmentNo">학과 코드</label>
					<input type="text" id="departmentNo" name="departmentNo" class="form-control" value="${professor.departmentNo }">
				</div>
				<div class="form-group">
					<label class="form-label" for="proHiredate">입사 일자</label>
					<input type="text" id="proHiredate" name="proHiredate" class="form-control" value="${professor.proHiredate }" readonly>
				</div>
				<div class="form-group">
					<label class="form-label" for="proRdate">퇴직 일자</label>
					<input type="text" id="proRdate" name="proRdate" class="form-control" value="${professor.proRdate }" readonly>
				</div>
				<div class="form-group">
					<label class="form-label" for="proStatus">교수 상태</label>
					<input type="text" id="proStatus" name="proStatus" class="form-control" value="${professor.proStatus }" readonly>
				</div>
				
				<div class="form-group">
					<label class="form-label" for="gender">성별</label>
					<input type="text" id="gender" name="user.gender" class="form-control" value="${professor.user.gender }" readonly>
				</div>
				
				<div class="form-group">
					<label class="form-label" for="userRegno1">주민등록번호 앞자리</label>
					<input type="text" id="userRegno1" name="user.userRegno1" class="form-control" value="${professor.user.userRegno1 }" readonly>
				</div>
				
				<div class="form-group">
					<label class="form-label" for="userRegno2">주민등록번호 뒷자리</label>
					<input type="text" id="userRegno2" name="user.userRegno2" class="form-control" value="${professor.user.userRegno2 }" readonly>
				</div>
				
				<div class="form-group">
					<label class="form-label" for="userTel">전화번호</label>
					<input type="text" id="userTel" name="user.userTel" class="form-control" value="${professor.user.userTel }">
				</div>
				
				<div class="form-group">
					<label class="form-label" for="userZip">우편번호</label>
					<input type="text" id="userZip" name="user.userZip" class="form-control" value="${professor.user.userZip }">
				</div>
				
				<div class="form-group">
					<label class="form-label" for="userAdd1">기본 주소</label>
					<input type="text" id="userAdd1" name="user.userAdd1" class="form-control" value="${professor.user.userAdd1 }">
				</div>
				
				<div class="form-group">
					<label class="form-label" for="userAdd2">상세 주소</label>
					<input type="text" id="userAdd2" name="user.userAdd2" class="form-control" value="${professor.user.userAdd2 }">
				</div>
				
				<div class="form-group">
					<label class="form-label" for="userEmail">이메일</label>
					<input type="text" id="userEmail" name="user.userEmail" class="form-control" value="${professor.user.userEmail }">
				</div>
				
				<div class="form-group">
					<label class="form-label" for="bankCd">은행코드</label>
					<input type="text" id="bankCd" name="user.bankCd" class="form-control" value="${professor.user.bankCd }">
				</div>
				
				<div class="form-group">
					<label class="form-label" for="userBankno">계좌번호</label>
					<input type="text" id="userBankno" name="user.userBankno" class="form-control" value="${professor.user.userBankno }">
				</div>
				
				<div>
					<button type="submit" class="btn btn-primary mb-4">Submit</button>
					<button type="reset" class="btn btn-danger mb-4">Reset</button>
				</div>
			</form>
		</div>
	</div>
</div>